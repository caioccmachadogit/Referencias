        android:icon="@drawable/ic"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="consumerwsvpro.view.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="consumerwsvpro.view.MenuActivity"></activity>
        <activity android:name="consumerwsvpro.view.StatusActivity"></activity>
        <activity android:name="consumerwsvpro.view.LigarActivity"></activity>
        <activity android:name="consumerwsvpro.view.DesligarActivity"></activity>
        <activity android:name="consumerwsvpro.view.ReiniciarActivity"></activity>
        <activity android:name="consumerwsvpro.view.DiscoActivity"></activity>
        <activity android:name="consumerwsvpro.view.MemoriaActivity"></activity>
        <activity android:name="consumerwsvpro.view.ProcessadorActivity"></activity>
        <activity android:name="consumerwsvpro.view.BiosActivity"></activity>
        <activity android:name="consumerwsvpro.view.MaquinaActivity"></activity>
        
    </application>

</manifest>
